{
    "version": "0.3.1",
    "browser": "dist/index.html",
    "scripts": {
        "dev": "parcel src/index.html",
        "build": "parcel build src/index.html --no-source-maps --no-scope-hoist"
    },
    "devDependencies": {
        "@parcel/transformer-less": "^2.0.0-beta.2",
        "@parcel/transformer-vue": "^2.0.0-beta.2",
        "babel-plugin-transform-glob-import": "^1.0.1",
        "less": "^4.1.2",
        "parcel": "^2.0.0-beta.2",
        "parcel-namer-without-hash": "^0.0.1",
        "vue": "^3.2.26",
        "vue-i18n": "^9.1.9"
    },
    "targets": {
        "browser": {
            "publicUrl": "./"
        }
    },
    "babel": {
        "plugins": [
            "babel-plugin-transform-glob-import"
        ]
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.4"
    }
